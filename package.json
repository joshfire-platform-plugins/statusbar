{
	"name":"statusbar",
	"fullName":"Status Bar",
	"version":"0.1.0",
	"keywords": [
       "mobile",
       "status",
       "bar",
       "header"
   ],
   "description":"Manage the visibility and style of the status bar on mobile platforms (currently iOS-only)",
   "joshfire":{
		"images":{
 	    	
	  },
	  "devices":["phone","tablet"],
 		"hooks":["xcodeproj", "startfile"],
 		"options":{
			"schema":{
        "style": {
          "type": "string",
          "title": "Status bar style",
          "enum":["none","black","default","translucent"],
          "default":"none"
        }
      },
			"form":[
				{
					"key":"style",
					"titleMap":{
            "none":"Hidden",
						"black":"Opaque black",
						"translucent":"Transparent black",
						"default":"Gray"
					}
				}
			]
		}
   }
}